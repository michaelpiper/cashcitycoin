{
  "name": "wallet",
  "version": "0.0.0",
  "description": "Minimalistic boilerplate to quick-start Node.js development in TypeScript.",
  "engines": {
    "node": ">= 14.15 <15"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/jest": "~26.0.15",
    "@types/node": "~14.14",
    "@typescript-eslint/eslint-plugin": "~4.7.0",
    "@typescript-eslint/parser": "~4.7.0",
    "eslint": "~7.13.0",
    "eslint-config-prettier": "~6.15.0",
    "eslint-plugin-jest": "~24.1.3",
    "jest": "~26.6.3",
    "prettier": "~2.1.2",
    "rimraf": "~3.0.2",
    "ts-jest": "~26.4.4",
    "tsutils": "~3.17.0",
    "typescript": "~4.0.5"
  },
  "scripts": {
    "start": "node application/server.js",
    "clean": "rimraf coverage build tmp",
    "build": "(rm -r application || echo \"application: No such file or directory\") && tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "postinstall": "tsc -p tsconfig.release.json",
    "watch-node": "nodemon application/server.js",
    "watch-ts": "tsc -w",
    "deploy": "git add . && git commit -m Heroku && git push heroku master"
  },
  "author": "Jakub Synowiec <github@jakubsynowiec.info>",
  "license": "Apache-2.0",
  "dependencies": {
    "@typegoose/typegoose": "^7.4.8",
    "@types/cookie-parser": "^1.4.2",
    "@types/kue": "^0.11.13",
    "@types/mongoose": "^5.10.0",
    "@types/redis": "^2.8.28",
    "@types/uuid": "^8.3.0",
    "@types/websocket": "^1.0.1",
    "axios": "^0.21.0",
    "bcrypt": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express-basic-auth": "^1.2.0",
    "firebase-admin": "^9.4.2",
    "kue": "^0.11.6",
    "mongoose": "^5.10.0",
    "redis": "^3.0.2",
    "tslib": "~2.0.3",
    "uuid": "^8.3.2",
    "websocket": "^1.0.33",
    "winston": "^3.3.3",
    "yup": "^0.32.8"
  },
  "volta": {
    "node": "14.15.0"
  }
}
